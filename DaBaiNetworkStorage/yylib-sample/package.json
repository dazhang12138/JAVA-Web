{
  "name": "yylib-sample",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "reg-cn":"npm config set registry https://registry.npm.taobao.org",
    "reg-clear":"npm config delete registry",
    "dev-SampleApp": "webpack-dev-server --devtool eval --display-error-details --progress --colors --hot --inline --config config.SampleApp.js ",
    "build-SampleApp": "set NODE_ENV=prod&&webpack --display-error-details --progress --colors -p --config config.SampleApp.js",
    "build-src-SampleApp": "set NODE_ENV=prod&&webpack --display-error-details --progress --colors -d --config config.SampleApp.js",
    "build-test-SampleApp": "set NODE_ENV=test&&webpack --display-error-details --progress --colors -p --config config.SampleApp.js",
    "build-test-src-SampleApp": "set NODE_ENV=test&&webpack --display-error-details --progress --colors -d --config config.SampleApp.js",
    "build-SampleApp-linux": "export NODE_ENV=prod&&webpack --display-error-details --progress --colors -p --config config.SampleApp.js",
    "build-src-SampleApp-linux": "export NODE_ENV=prod&&webpack --display-error-details --progress --colors -d --config config.SampleApp.js",
    "build-test-SampleApp-linux": "export NODE_ENV=test&&webpack --display-error-details --progress --colors -p --config config.SampleApp.js",
    "build-test-src-SampleApp-linux": "export NODE_ENV=test&&webpack --display-error-details --progress --colors -d --config config.SampleApp.js",
    "dll-SampleApp": "webpack --display-error-details --progress --colors -p --config config.SampleApp.dll.js",
    "dll-src-SampleApp": "webpack --display-error-details --progress --colors -d --config config.SampleApp.dll.js"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-core": "6.9.1",
    "babel-loader": "6.2.4",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0":"6.24.1",
    "bundle-loader": "0.5.4",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "gulp": "3.9.0",
    "gulp-concat": "2.6.0",
    "gulp-less": "3.1.0",
    "gulp-replace": "0.5.4",
    "html-webpack-plugin": "2.22.0",
    "json-loader": "0.5.4",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "postcss-loader": "0.13.0",
    "react-hot-loader": "3.0.0-beta.6",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.1",
    "style-loader": "0.12.4",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "yylib-react":">=0.86.0",
    "yylib-cscec":">=0.1.58"
  }
}
